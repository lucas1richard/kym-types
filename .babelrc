{
  "presets": [
      ["@babel/env", {
        "targets": {
          "node": "current"
        }
      }],
      "@babel/typescript"
  ],
  "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/proposal-object-rest-spread",
      ["module-resolver", {
        "root": ["./src"]
      }]
  ]
}